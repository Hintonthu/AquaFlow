{
    "program": "aqua_flow.p4",
    "language": "p4-16",
    "targets": {
      "multiswitch": {
          "auto-control-plane": true,
          "cli": false,
          "pcap_dump": true,
          "bmv2_log": true,
          "links": [["h1", "s1", "5ms"], ["s1", "s2", "5ms"], ["s1", "s3", "5ms"], ["s1", "s4", "5ms"],
                    ["h2", "s5", "5ms"], ["s2", "s5", "5ms"], ["s3", "s5", "5ms"], ["s4", "s5", "5ms"]],
          "hosts": {
            "h1": {
                "cmd": "sudo python /home/rakesh/AquaFlow/send_stream.py --npackets 100 --type diversity --payload 1024",
                "wait": false
            },
            "h2": {
                "cmd": "sudo python /home/rakesh/AquaFlow/receive_stream.py --iface h2-eth0 --npackets 100 --payload 1024 --log_file /home/rakesh/AquaFlow/experiment/diversity_payload_1024_differential_0.json",
                "wait": true
            }
          },
          "switches": {
              "s1": {
                  "entries": "diversity-commands/s1-commands.txt"
              },
              "s2": {
                  "entries": "diversity-commands/s2-commands.txt"
              },
              "s3": {
                  "entries": "diversity-commands/s3-commands.txt"
              },
              "s4": {
                  "entries": "diversity-commands/s4-commands.txt"
              },
              "s5": {
                  "entries": "diversity-commands/s5-commands.txt"
              }
          }
       }
    }
}
